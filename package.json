{
  "name": "robo_oshw_bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && babel --out-dir dist src",
    "build:dev": "npm run clean && ENV=dev babel --out-dir dist src",
    "bot:dev": "npm run build:dev && ENV=dev node dist/index.js",
    "start": "nodemon src/index.js --exec babel-node --ignore './files/**'",
    "start:server": "nodemon src/server.js --exec babel-node --ignore './files/**'",
    "cron": "nodemon src/cron.js --exec babel-node --ignore './files/**'",
    "cron:dev": "ENV=dev nodemon src/cron.js --exec babel-node --ignore './files/**'",
    "pretty": "prettier --write \"./src/**/*.{js,ts,json}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@telegraf/entity": "^0.4.0",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.6",
    "stripe": "^14.5.0",
    "telegraf": "^4.15.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.3",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.3",
    "core-js": "^3.33.3",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0"
  }
}
